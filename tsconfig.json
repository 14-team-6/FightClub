{
  "compilerOptions": {
      "outDir": "dist",
      "target": "es2016",
      "declaration": false,
      "module": "commonjs",
      "strictNullChecks": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "jsx": "react",
      "baseUrl": "./",
      "paths": {
        "@f/*": ["frontend/*"],
        "@fcomponents/*": ["frontend/src/components/*"],
        "@fpublic/*": ["frontend/public/*"],
        "@flayouts/*": ["frontend/src/layouts/*"]
      }
  },
  "exclude": [
    "node_modules"
  ]
}
